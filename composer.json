{
    "name": "goodmain/telescope-aggregate",
    "description": "Laravel library for collecting aggregated data from the telescope by numerous periods",
    "keywords": ["telescope", "laravel", "aggregation"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vitaly Iskrin",
            "email": "iskrinvv@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0",
        "laravel/telescope": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Goodmain\\TelescopeAggregate\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Goodmain\\TelescopeAggregate\\TelescopeAggregateServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
